---
import TripLayout from '../../layouts/TripLayout.astro';
import { render, getEntry } from 'astro:content';

const entry = await getEntry('japan2026', 'practical-info');
const { Content } = await render(entry!);
---

<TripLayout title="Practical Info" activeNav="practical-info">
  <div class="content-card prose">
    <Content />
  </div>
</TripLayout>
